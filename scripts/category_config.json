  "parserTypes": {
    "standard": {
      "description": "標準パーサー（item_currencyクラス）",
      "selectors": ["a.item_currency"],
      "extractionStrategy": "linkText"
    },
    "augment": {
      "description": "オーグメント専用パーサー（複数のセレクタ対応）",
      "selectors": [
        "a.item_currency",
        "table.itemDataTable a[href^='/jp/']",
        "a.whiteitem"
      ],
      "extractionStrategy": "linkTextOrTableCell"
    },
    "omen": {
      "description": "オーメン専用パーサー",
      "selectors": ["a.whiteitem.Omen"],
      "extractionStrategy": "linkText"
    },
    "unique": {
      "description": "ユニークアイテム専用パーサー",
      "selectors": ["a.uniqueitem"],
      "extractionStrategy": "uniqueNameSpan"
    },
    "trial": {
      "description": "試練関連パーサー",
      "selectors": [
        "a.item_currency",
        "a.whiteitem",
        "table a[href^='/jp/']"
      ],
      "extractionStrategy": "linkTextOrTableCell"
    },
    "monolith": {
      "description": "モノリス/ピナクルキー専用パーサー",
      "selectors": [
        "a.item_currency",
        "a.whiteitem",
        "a.uniqueitem",
        "table a[href^='/jp/']"
      ],
      "extractionStrategy": "flexible"
    },
    "gem": {
      "description": "ジェム専用パーサー（スキル/サポート）",
      "selectors": [
        "table a[href^='/jp/']",
        "a.gemitem",
        "td a[href*='Gem']"
      ],
      "extractionStrategy": "linkTextOrTableCell"
    },
    "waystone": {
      "description": "ウェイストーン/マップ専用パーサー",
      "selectors": [
        "table a[href^='/jp/']",
        "a.item_currency",
        "td a"
      ],
      "extractionStrategy": "linkTextOrTableCell"
    }
  }
}
